<h2>Admin Panel</h2>
<h3>Users</h3>
<ul>
  <% users.forEach(user => { %>
    <li><%= user.username %> - <%= user.isAdmin ? 'Admin' : 'User' %></li>
  <% }) %>
</ul>

<h3>Bots</h3>
<ul>
  <% bots.forEach(bot => { %>
    <li><%= bot.appName %> - Owned by: <%= bot.owner.username %></li>
  <% }) %>
</ul>

<a href="/dashboard">Back to Dashboard</a>
